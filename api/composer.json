{
    "name": "flaaim/auction",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 0,
        "sort-packages":true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "doctrine/annotations": "^2.0",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "laminas/laminas-config-aggregator": "^1.9",
        "monolog/monolog": "^2.10",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^4.2",
        "sentry/sentry": "^4.16",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.14",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.0",
        "symfony/mailer": "^5.4",
        "twig/twig": "^3.11",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.5.0",
        "doctrine/data-fixtures": "^1.6",
        "overtrue/phplint": "^3.4.0",
        "phpunit/phpunit": "^9.6",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^5.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "app": "php bin/app.php --ansi",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-html var/test/coverage"
    }
}
